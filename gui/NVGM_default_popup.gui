types NVGM_default_popup_types {	
	type nvgm_default_popup = default_popup
	{
		layer = top
		visible = "[GetVariableSystem.Exists('show_nvgm_default_popup')]"
		blockoverride "window_header_name" {
			text = "NVGM Menu"
		}
		blockoverride "header_close_button_visibility" {
			visible = yes
		}
		blockoverride "header_close_button" {
			onclick = "[GetVariableSystem.Toggle('show_nvgm_default_popup')]"
		}
		blockoverride "scrollarea_content" {
			flowcontainer = {
				resizeparent = yes
				direction = vertical
				maximumsize = { 700 -1 }
				minimumsize = { 700 -1 }
			}
		}
		blockoverride "fixed_bottom" {
			flowcontainer = {
				using = default_list_position
				direction = vertical
				flowcontainer = {
					direction = horizontal
					using = default_list_position
					flowcontainer = {
						textbox = {
							raw_text = "Global Data"
							autoresize = yes
							margin = { 10 10 }
							multiline = yes
						}
						widget = {
							size = { 60 44 }
							checkbutton = {
								using = check_button
								size = { 24 24 }
								parentanchor = vcenter|left
								checked = "[GetVariableSystem.Exists('show_nvgm_data_popup')]"
								onclick = "[GetVariableSystem.Toggle('show_nvgm_data_popup')]"
								onclick = "[ExecuteConsoleCommand('event nvgm_ranking_chart.3')]"
							}
						}
					}
					flowcontainer = {
						textbox = {
							raw_text = "NVGM_RANKING_CHART"
							autoresize = yes
							margin = { 10 10 }
							multiline = yes
						}
						widget = {
							size = { 60 44 }
							checkbutton = {
								using = check_button
								size = { 24 24 }
								parentanchor = vcenter|left
								checked = "[GetVariableSystem.Exists('show_nvgm_ranking_chart_popup')]"
								onclick = "[ExecuteConsoleCommand('event nvgm_ranking_chart.1')]"
								onclick = "[GetVariableSystem.Toggle('show_nvgm_ranking_chart_popup')]"
								onclick = "[GetVariableSystem.Toggle('show_not_nvgm_1_ranking')]"
								onclick = "[GetVariableSystem.Toggle('show_not_nvgm_2_ranking')]"
								onclick = "[GetVariableSystem.Toggle('show_not_nvgm_3_ranking')]"
								onclick = "[GetVariableSystem.Toggle('show_not_nvgm_4_ranking')]"
								onclick = "[GetVariableSystem.Toggle('show_not_nvgm_5_ranking')]"
								onclick = "[GetVariableSystem.Toggle('show_not_nvgm_6_ranking')]"
								onclick = "[GetVariableSystem.Toggle('show_not_nvgm_7_ranking')]"
								onclick = "[GetVariableSystem.Toggle('show_not_nvgm_8_ranking')]"
								onclick = "[GetVariableSystem.Toggle('show_not_nvgm_9_ranking')]"
								onclick = "[GetVariableSystem.Toggle('show_not_nvgm_10_ranking')]"
								onclick = "[GetVariableSystem.Toggle('show_not_nvgm_my_ranking')]"
							}
						}
					}
					flowcontainer = {
						textbox = {
							raw_text = "Subject Menu"
							autoresize = yes
							margin = { 10 10 }
							multiline = yes
						}
						widget = {
							size = { 60 44 }
							checkbutton = {
								using = check_button
								size = { 24 24 }
								parentanchor = vcenter|left
								checked = "[GetVariableSystem.Exists('show_nvgm_subject_popup')]"
								onclick = "[GetVariableSystem.Toggle('show_nvgm_subject_popup')]"
