									fontsize = 17
									parentanchor = vcenter
									align = nobaseline|left
									raw_text = "[Country.GetInvestmentPool|vD]"
								}
							}
							widget = {
								textbox = {
									position = { 120 20 }
									size = { 550 14 }
									fontsize = 17
									parentanchor = vcenter
									align = nobaseline|right
